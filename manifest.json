{
  "manifest_version": 2,
  "name": "Layers viewer",
  "description": "This extension show layers off div",
  "version": "0.1",
    "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
   "default_title": "Show borders of layers"
  },
 "permissions": [
   "activeTab"
 ]
}